<!doctype html>
<html lang="NO">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge">
  {{{decorator.STYLES}}}
  {{#each podlets.vta.css}}
    {{this}}
  {{/each}}
  {{#each podlets.vta.js}}
    {{this}}
  {{/each}}
  {{#each podlets.vtaSituasjon.css}}
    {{this}}
  {{/each}}
  {{#each podlets.vtaSituasjon.js}}
    {{this}}
  {{/each}}
  <title>{{title}}</title>
</head>
<body>
<noscript>
  You need to enable JavaScript to run this app.
</noscript>

{{{decorator.HEADER}}}
<div id="podlet-vta">
  {{{podlets.vta}}}
</div>
<div id="podlet-vta-situasjon">
  {{{podlets.vtaSituasjon}}}
</div>
{{{decorator.FOOTER}}}
{{{decorator.SCRIPTS}}}
</body>
